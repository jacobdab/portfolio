<% include ../partials/headersite%>


<section class="addDevice d-flex justify-content-center align-items-center mt-3 h-100 w-100">
    <div class="container">
        <div class="form-box w-50 mx-auto">
            <form method="post" action="/shop/<%=id%>/addSubCategory">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="form-group">
                            <input type="text" class="form-control" name="subCategory" placeholder='Podkategoria' required>
                        </div>
                        <div class="form-group">
                            <input type="url" class="form-control" name="imageTH" placeholder='Link do obrazka' required>
                        </div>
                        <button type="submit" class="btn btn-primary">Dodaj kategorie</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>



