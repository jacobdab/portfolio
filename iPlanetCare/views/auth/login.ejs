<% include ../partials/headersite%>
        <section class="login d-flex justify-content-center align-items-center mt-3 h-100 ">
            <div class="form-box w-25 ">
                <form method="post" action="/login">
                    <div class="form-group">
                        <input type="text" class="form-control" id="username" name="username" aria-describedby="emailHelp" placeholder="Username">
                    </div>
                    <div class="form-group">
                        <input type="password" class="form-control" id="haslo" name="password" placeholder="Password">
                    </div>
                    <button type="submit" class="btn btn-primary w-100">Sign in</button>
                </form>
                <a href="/forgot" class="btn btn-info btn-xsn">Reset password</a>
            </div>

        </section>


